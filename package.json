    "eslint": "^9.19.0",
    "globals": "^15.14.0",
    "typescript": "5.7.3"
  },
  "repository": "scttcper/eslint-config-biome",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "publishConfig": {
